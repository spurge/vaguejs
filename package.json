{
  "name": "vague",
  "version": "0.1.1",
  "description": "Vaguejs is a library for creating data for fuzzy tests",
  "main": "dist",
  "typings": "dist",
  "scripts": {
    "build": "tsc",
    "clean": "rm -Rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "nyc mocha -r ts-node/register -r source-map-support/register src/**/*.spec.ts"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/*.spec.ts",
      "**/*.js"
    ],
    "all": true
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spurge/vaguejs.git"
  },
  "keywords": [
    "testing",
    "test",
    "fuzzy"
  ],
  "author": "spurge",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spurge/vaguejs/issues"
  },
  "homepage": "https://github.com/spurge/vaguejs#readme",
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/lodash": "^4.14.104",
    "@types/mocha": "^2.2.48",
    "@types/node": "^9.4.6",
    "chai": "^4.1.2",
    "mocha": "^5.0.1",
    "nyc": "^11.6.0",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^2.7.2"
  },
  "dependencies": {
    "lodash": "^4.17.5"
  }
}
